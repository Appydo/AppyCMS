RewriteEngine On
RewriteCond %{HTTP_HOST} ^breizhadonf.com [NC]
RewriteRule ^(.*)$ http://www.breizhadonf.com/$1 [L,R=301]
RewriteCond %{HTTP_HOST} ^rbtcreation.com [NC]
RewriteRule ^(.*)$ http://www.rbtcreation.com/$1 [L,R=301]
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]
RewriteRule ^.*$ index.php [NC,L]
rewriteRule ^index\.php\?pageSelect=galerie$ http://galerie.rbtcreation.com/ [R=permanent,L]